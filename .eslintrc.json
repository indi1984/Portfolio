{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "google",
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
        "plugins": [
            "ejs"
        ],
    "rules": {
        "max-len": [
            "error",
            {
                "code": 160
            }
        ],
        "require-jsdoc": "off",
        "new-cap": "off"
    }
}